RewriteEngine On
RewriteRule ^tag/([a-zA-Z0-9\(\)_-]+)$ algoGrab.php?type=tag&keyword=$1
RewriteRule ^tag/([a-zA-Z0-9\(\)_-]+)/$ algoGrab.php?type=tag&keyword=$1
RewriteRule ^category/([a-zA-Z0-9\(\)_-]+)$ algoGrab.php?type=category&keyword=$1
RewriteRule ^category/([a-zA-Z0-9\(\)_-]+)/$ algoGrab.php?type=category&keyword=$1
RewriteRule ^algo/([a-zA-Z0-9\(\)_-]+)$ algoGrab.php?type=algo&keyword=$1
RewriteRule ^algo/([a-zA-Z0-9\(\)_-]+)/$ algoGrab.php?type=algo&keyword=$1

